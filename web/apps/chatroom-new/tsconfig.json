{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "lib": ["es2021", "dom"],
    "declaration": false,
    "rootDir": ".",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*.ts","src/**/*.js", "test/**/*.ts"]
}